{
  "name": "backend",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev --name init",
    "db:deploy": "npx prisma migrate deploy && npm run db:generate",
    "studio": "npx prisma studio",
    "lint": "eslint .",
    "test:unit": "mocha tests/unit/**/*.test.js --exit",
    "test:integration": "cross-env NODE_ENV=test mocha tests/integration/**/*.test.js --exit",
    "coverage": "nyc npm run test:unit"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "bcryptjs": "^3.0.2",
    "chai": "^4.5.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "proxyquire": "^2.1.3",
    "sinon": "^21.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "chai-http": "^5.1.2",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.30.0",
    "mocha": "^11.7.1",
    "nodemon": "^3.1.10",
    "nyc": "^17.1.0",
    "prisma": "^6.10.1",
    "supertest": "^7.1.4"
  }
}
